#lang pie

(claim + (-> Nat Nat Nat))
(define + (lambda (a b) (
                         iter-Nat a b (lambda (x) (add1 x))
                         )))

(claim * (-> Nat Nat Nat))
(define * (lambda (a b) (
                         rec-Nat a 0 (lambda (curr acc) (+ acc b))
                         )))

